{
  "name": "gcp-quiz-modern",
  "version": "2.0.0",
  "description": "Modern GCP Quiz Application with Advanced Features",
  "main": "quiz-parts/index.html",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "npx http-server . -p 8080 -o",
    "serve": "python3 -m http.server 8080",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.js,.html --fix"
  },
  "keywords": [
    "gcp",
    "quiz",
    "typescript",
    "vite",
    "modern"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.4.0",
    "lucide": "^0.294.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "terser": "^5.43.1",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
