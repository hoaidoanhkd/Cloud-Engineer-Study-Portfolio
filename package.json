{
  "name": "gcp-quiz-static",
  "version": "1.0.0",
  "description": "GCP Quiz Static Website",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server -p $PORT -o",
    "build": "node -e \"const fs=require('fs');const path=require('path');if(!fs.existsSync('dist'))fs.mkdirSync('dist');fs.copyFileSync('index.html','dist/index.html');fs.copyFileSync('README.md','dist/README.md');\"",
    "dev": "npx http-server -p 3000 -o"
  },
  "keywords": ["gcp", "quiz", "static"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 